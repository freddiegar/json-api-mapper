{
  "name": "freddiegar/json-api-mapper",
  "description": "Mapper response json-api in PHP",
  "keywords": [
    "jsonapi.org",
    "json-api",
    "jsonapi",
    "json",
    "mapping",
    "mapper",
    "map",
    "api"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.1.3",
    "dflydev/dot-access-data": "v2.0.*"
  },
  "autoload": {
    "psr-4": {
      "FreddieGar\\JsonApiMapper\\": "src/"
    }
  },
  "require-dev": {
    "phpmd/phpmd": "^2.8",
    "phpstan/phpstan": "~0.9",
    "phpunit/phpunit": "^7.0",
    "squizlabs/php_codesniffer": "~3.2"
  },
  "autoload-dev": {
    "psr-4": {
      "FreddieGar\\JsonApiMapper\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "@phpcs",
      "@phpmd",
      "@phpstan",
      "@phpunit"
    ],
    "phpcs": "phpcs --standard=PSR2 --ignore=tests/coverage src/ tests/",
    "phpmd": "phpmd src/ ansi naming,unusedcode",
    "phpstan": "phpstan analyse -l 1 src/ tests/",
    "phpunit": "phpunit --bootstrap vendor/autoload.php --testdox tests/"
  },
  "config": {
    "sort-packages": true,
    "classmap-authoritative": true,
    "platform": {
        "php": "7.1.3"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
